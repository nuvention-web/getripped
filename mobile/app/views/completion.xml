<Alloy>
	<NavigationWindow id="compNavWin">
	<Window id="exWin" backgroundColor="#F6F6F6" title="SwoleTrain">
		<ScrollView id="scrollviewId" scrollingEnabled="true" showVerticalScrollIndicator="true">
		<View id="mainView" layout="vertical">
			<View id = "topView">
				<Label id="topLabel"></Label>
			</View>
			<View id="view1">
				<TableView id="workoutsTable" class="workoutTable" height="SIZE" backgroundColor="transparent">		
				</TableView>
			</View>
			<View id = "bottomView">
				<Button id="btnDashboard" top="20" title="Go to Dashboard" onClick="showDashboard"></Button>
			</View>
			</View>		
		</ScrollView>
	</Window>
	</NavigationWindow>
</Alloy>